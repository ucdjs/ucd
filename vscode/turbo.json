{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": [
        "ext:gen"
      ],
      "outputs": [
        "dist/**",
        "src/generated/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": false
    },
    "typecheck": {
      "outputs": [".cache/tsbuildinfo.json"]
    }
  }
}
