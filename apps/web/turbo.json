{
  "$schema": "https://turborepo.dev/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".output/**"
      ]
    },
    "dev": {
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [".cache/tsbuildinfo.json"]
    }
  }
}
