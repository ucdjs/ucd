{
  "$schema": "https://turborepo.dev/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalPassThroughEnv": [
    "NODE",
    "NODE_ENV",
    "CI",
    "CLOUDFLARE_ENV"
  ],
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "@ucdjs/api#build:openapi",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ]
    },
    "//#test": {
      "dependsOn": []
    },
    "test": {
      "cache": false,
      "dependsOn": []
    }
  }
}
