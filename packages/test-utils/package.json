{
  "name": "@ucdjs/test-utils",
  "version": "1.0.0",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.26.2",
  "license": "MIT",
  "homepage": "https://github.com/ucdjs/ucd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucdjs/ucd.git",
    "directory": "packages/test-utils"
  },
  "bugs": {
    "url": "https://github.com/ucdjs/ucd/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./fs-bridges": "./dist/fs-bridges/index.mjs",
    "./matchers/types": "./dist/matchers/types.mjs",
    "./matchers/vitest-setup": "./dist/matchers/vitest-setup.mjs",
    "./mock-store": "./dist/mock-store.mjs",
    "./msw": "./dist/msw.mjs",
    "./msw/vitest-setup": "./dist/msw/vitest-setup.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.18"
  },
  "scripts": {
    "build": "tsdown --tsconfig=./tsconfig.build.json",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf dist node_modules",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "generate:client": "openapi-typescript ../../ucd-generated/api/openapi.json -o ./src/.generated/api.d.ts",
    "generate:client:local": "openapi-typescript http://localhost:8787/openapi.json -o ./src/.generated/api.d.ts"
  },
  "peerDependencies": {
    "@ucdjs/ucd-store": "workspace:*",
    "vitest-testdirs": "catalog:testing"
  },
  "dependencies": {
    "@luxass/msw-utils": "catalog:prod",
    "@luxass/utils": "catalog:prod",
    "@ucdjs-internal/shared": "workspace:*",
    "@ucdjs/fs-bridge": "workspace:*",
    "@ucdjs/schemas": "workspace:*",
    "msw": "catalog:testing",
    "zod": "catalog:prod"
  },
  "devDependencies": {
    "@luxass/eslint-config": "catalog:linting",
    "@ucdjs-tooling/tsconfig": "workspace:*",
    "@ucdjs-tooling/tsdown-config": "workspace:*",
    "@ucdjs/ucd-store": "workspace:*",
    "@vitest/expect": "catalog:testing",
    "eslint": "catalog:linting",
    "openapi-typescript": "catalog:dev",
    "publint": "catalog:dev",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest-testdirs": "catalog:testing"
  },
  "publishConfig": {
    "access": "public"
  }
}
