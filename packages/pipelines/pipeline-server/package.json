    "@ucdjs/pipelines-ui": "workspace:*",
    "chokidar": "catalog:prod",
    "crossws": "catalog:prod",
    "esrap": "2.2.2",
    "h3": "catalog:prod",
    "oxc-parser": "0.112.0",
